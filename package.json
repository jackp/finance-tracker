{
  "name": "finance-tracker",
  "version": "0.0.1",
  "description": "Simple finance tracker, showing net income per day/week/month. Example architecture for stand-alone React application",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_PATH=./src webpack-dev-server --hot --config webpack.config.babel.js",
    "build": "NODE_PATH=./src webpack --config webpack.prod.config.babel.js",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackp/finance-tracker.git"
  },
  "keywords": [
    "react",
    "finance"
  ],
  "author": "Jack Parker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackp/finance-tracker/issues"
  },
  "homepage": "https://github.com/jackp/finance-tracker#readme",
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.15.0",
    "file-loader": "^0.8.5",
    "gh-pages": "^0.8.0",
    "html-webpack-plugin": "^1.7.0",
    "node-sass": "^3.4.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^0.14.6"
  }
}
